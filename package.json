{
  "name": "qr-attendance-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "NODE_OPTIONS=--dns-result-order=ipv4first node dist/server.js",
    "generate_qr": "ts-node src/utils/generateEventQRCodes.ts",
    "seeds": "ts-node src/seeds.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "moment": "2.30.1",
    "pdfkit": "0.17.2",
    "pg": "^8.11.0",
    "qrcode": "1.5.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.4",
    "@types/pdfkit": "0.17.3",
    "@types/pg": "8.15.5",
    "@types/qrcode": "1.5.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
